<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rschina.heilongjiang">
    <uses-permission android:name="android.permission.READ_PHONE_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" >
    </uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" >
    </uses-permission>
    <uses-permission android:name="android.permission.WRITE_SETTINGS" >
    </uses-permission>
    <application

        android:allowBackup="true"
        android:icon="@drawable/heilongjiang"

        android:name="com.rschina.heilongjiang.MyApplication"
        android:label="@string/app_name"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">

        <activity android:name="com.rschina.heilongjiang.MainActivity"

            android:launchMode="singleTask"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.rschina.heilongjiang.activity.FristChooseActivity"/>
        <activity android:name="com.rschina.heilongjiang.activity.ReportActivity"/>
        <activity android:name="com.rschina.heilongjiang.activity.SecondChooseActivity"/>
        <activity android:name="com.rschina.heilongjiang.activity.CompareChooseActivity"/>
        <activity android:name="com.rschina.heilongjiang.activity.HelpActivity"/>
        <activity android:name="com.rschina.heilongjiang.activity.BaseChooseActivity"/>
        <activity android:name=".activity.UserActivity"/>
        <activity android:name=".activity.MarkActivity"/>
        <activity android:name=".activity.RegistActivity"/>
        <activity android:name=".activity.FeadBackActivity"/>
        <activity android:name=".activity.SettingActivity"/>
        <activity android:name=".activity.ForgetActivity"/>
        <activity android:name=".activity.ForgettwoActivity"/>
        <activity android:name=".activity.CollectionActivity"/>
    </application>

</manifest>